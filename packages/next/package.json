{
  "name": "next",
  "scripts": {
    "build": "next build",
    "check-all": "run-s fix check-code test",
    "check-code": "run-p spell-check lint type-check",
    "dev": "next dev",
    "export-check": "ts-unused-exports ./tsconfig.json --searchNamespaces",
    "fix": "run-p fix:*",
    "fix-ts-by-eslint": "eslint \"./src/**/*.{ts,tsx}\" --fix",
    "fix-ts-by-prettier": "prettier \"./src/**/*.{ts,tsx}\" --write",
    "fix:config": "prettier \"./*.{js,json}\" \"./**/*.js\" --write",
    "fix:css": "stylelint \"./src/**/*.{ts,tsx}\" --fix",
    "fix:pack": "fixpack",
    "fix:ts": "run-s fix-ts-by-eslint fix-ts-by-prettier",
    "lint": "run-p lint:*",
    "lint:css": "stylelint \"./src/**/*.{ts,tsx}\"",
    "lint:ts": "eslint \"./src/**/*.{ts,tsx}\"",
    "spell-check": "cspell \"./src/**/*.{ts,tsx}\"",
    "start": "next start",
    "test": "echo \"jest は cli の表示をリセットしてしまうため、 check-all の後ろに直列でつなげる\"",
    "type-check": "tsc"
  },
  "dependencies": {
    "@types/styled-components": "^5.1.15",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/react": "17.0.30",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "cspell": "^5.12.3",
    "esbuild-jest": "^0.5.0",
    "eslint": "8.0.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^37.0.1",
    "fixpack": "^4.0.0",
    "jest": "^27.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-unused-exports": "^7.0.3",
    "typescript": "4.4.4"
  },
  "version": "0.1.0",
  "private": true
}
